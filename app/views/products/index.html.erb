<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>

<section class="products">

<% @products.each do |product| %>
  <article class="product_details">
    <ul>
      <li>
        <%= product.name %>
        <%= product.price %>
        <%= product.description %>
        <%= product.user %>
      </li>
    </ul>
  <% end %>
  </article>


</section>
